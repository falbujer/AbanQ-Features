////////////////////////////////////////////////////////////////////////////
//// DEFINICION ////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////

//////////////////////////////////////////////////////////////////
//// INTERNA /////////////////////////////////////////////////////

function interna_init()
{
}

//// INTERNA /////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition calidadProv */
/////////////////////////////////////////////////////////////////
//// CALIDAD_PROV /////////////////////////////////////////////////

function calidadProv_init()
{
	var util:FLUtil = new FLUtil();
	
	var codProveedor:String = this.cursor().valueBuffer("codproveedor");
	var nivelAnterior:String = util.sqlSelect("proveedores", "nivelactual", "codproveedor = '" + codProveedor + "'");
	this.cursor().setValueBuffer("nivelanterior", nivelAnterior);
}

//// CALIDAD_PROV /////////////////////////////////////////////////
