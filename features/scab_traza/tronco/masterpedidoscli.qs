//// SCAB_TRAZA /////////////////////////////////////////////////
class scabTraza extends lotes {
    function scabTraza( context ) { lotes ( context ); }
	function generarAlbaran(where:String, cursor:FLSqlCursor):Number {
		return this.ctx.scabTraza_generarAlbaran(where, cursor);
	}
}
//// SCAB_TRAZA /////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition scabTraza */
/////////////////////////////////////////////////////////////////
//// SCAB_TRAZA /////////////////////////////////////////////////
function scabTraza_generarAlbaran(where:String, cursor:FLSqlCursor):Number
{
	var idAlbaran:String = this.iface.__generarAlbaran(where, cursor);
	if (!idAlbaran) {
		return false;
	}
	return idAlbaran;
}
