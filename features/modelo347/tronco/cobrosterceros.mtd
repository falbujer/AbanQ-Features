		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>2</length>
		<calculated>true</calculated>
		<editable>false</editable>
	</field>

	<field>
		<name>cuenta</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nº Cuenta")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>
		<editable>false</editable>
	</field>
	
	<field>
        <name>nomodelo347</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Excluir de 347")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>bool</type>
    </field>
	
	<field>
		<name>idasientocobro</name>
		<!-- Código del asiento contable generado por el pago o devolución-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Asiento Cobro")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>
	
		<relation>
			<table>co_asientos</table>
			<field>idasiento</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>idasientopago</name>
		<!-- Código del asiento contable generado por el pago o devolución-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Asiento Pago")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>
	
		<relation>
			<table>co_asientos</table>
			<field>idasiento</field>
			<card>M1</card>
		</relation>
