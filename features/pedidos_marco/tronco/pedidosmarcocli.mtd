		<null>false</null>
		<pk>true</pk>
		<type>string</type>
		<length>8</length>
		<counter>true</counter>

		<relation>
			<table>lineaspedidomarcocli</table>
			<field>codpedidomarco</field>
			<card>1M</card>
		</relation>
	</field>

	<field>
