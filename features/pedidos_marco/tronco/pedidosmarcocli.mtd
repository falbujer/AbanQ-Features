		<partI>8</partI>
		<partD>2</partD>
		<default>0</default>
		<editable>false</editable>
	</field>
	
	<field>
		<name>totalpedido</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","T.Pedido")</alias>
		<null>false</null>
