
/** @class_declaration produccion */
/////////////////////////////////////////////////////////////////
//// PRODUCCION /////////////////////////////////////////////////
class produccion extends oficial {
    function produccion( context ) { oficial ( context ); }
    function obtenerParamInforme() {
		return this.ctx.produccion_obtenerParamInforme();
	}
}
//// PRODUCCION /////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition produccion */
/////////////////////////////////////////////////////////////////
//// PRODUCCION /////////////////////////////////////////////////
function produccion_obtenerParamInforme()
{
	var _i = this.iface;
	
	var pI = _i.__obtenerParamInforme();
	
	if(pI.whereFijo != "")
		pI.whereFijo += " AND ";
	
	pI.whereFijo += "articulos.tipostock <> 'Sin stock'";	
	
	return pI;
}
//// PRODUCCION /////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////
