		<name>ivaincluido</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Facturar con I.V.A. incluido")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>riesgomax</name>
		<!--Cantidad máxima que el cliente podrá adeudar a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Riesgo máximo autorizado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>riesgoalcanzado</name>
		<!--Cantidad máxima que el cliente ha adeudado a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Riesgo alcanzado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
		<editable>false</editable>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>capitalimpagado</name>
		<!--Cantidad impagada por el cliente a la empresa-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Capital impagado")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>8</partI>
		<partD>2</partD>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>copiasfactura</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Copias por factura")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>uint</type>
		<default>1</default>
		<visiblegrid>false</visiblegrid>
	</field>

