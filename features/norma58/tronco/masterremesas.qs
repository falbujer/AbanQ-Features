class norma58 extends oficial {
    function norma58( context ) { oficial ( context ); }
	function init() {
		return this.ctx.norma58_init();
	}
	function volcarADisco58() {
		return this.ctx.norma58_volcarADisco58();
	}
}
//// NORMA_58 ///////////////////////////////////////////////////
