		<pk>false</pk>
		<type>string</type>
		<length>25</length>
		
		<relation>
			<table>tiposenvases</table>
			<field>codtipoenvase</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>cantidad</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Cantidad")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>int</type>
	</field>
	
