	<field>
		<name>codpasarela</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>string</type>
		<length>15</length>
		
		<relation>
			<table>parametrospasarela</table>
			<field>codpasarela</field>
			<card>1M</card>
		</relation>
		
		<relation>
			<table>formaspago</table>
			<field>codpasarela</field>
			<card>1M</card>
		</relation>
	</field>
	
	<field>
		<name>nombre</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre")</alias>
		<null>false</null>
		<pk>false</pk>
