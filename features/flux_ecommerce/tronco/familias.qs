/** @class_definition fluxEcommerce */
/////////////////////////////////////////////////////////////////
//// FLUX ECOMMERCE //////////////////////////////////////////////////////
function fluxEcommerce_init()
{
	this.iface.__init();
	if (this.cursor().modeAccess() == this.cursor().Insert) {
		this.child("tdbFamiliasHijas").cursor().setMainFilter("codmadre = '-1'");
		this.child("tdbFamiliasHijas").refresh();
	}
}
