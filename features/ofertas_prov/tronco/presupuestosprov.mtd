<!DOCTYPE TMD>
<TMD>
    <name>presupuestosprov</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Ofertas de proveedores")</alias>
    
    <field>
        <name>idpresupuesto</name>
		<!--Identificador interno de la tabla-->        <alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
        <null>false</null>
        <pk>true</pk>
        <type>serial</type>
        <visiblegrid>false</visiblegrid>
      <relation>
            <table>lineaspresupuestosprov</table>
            <field>idpresupuesto</field>
            <card>1M</card>
        </relation>
      <relation>
            <table>datosoferta</table>
            <field>idpresupuestoprov</field>
            <card>1M</card>
        </relation>
      <relation>
            <table>lineassolpresupuestosprov</table>
            <field>idpresupuestoprov</field>
            <card>1M</card>
        </relation>
    </field>
    <field>
        <name>codigo</name>
		<!--Código asociado al presupuesto-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Nº oferta")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>string</type>
        <length>12</length>
        <default>0</default>
        <editable>false</editable>
        <relation>
            <table>i_presupuestosprov</table>
            <field>d_presupuestosprov_codigo</field>
            <card>1M</card>
        </relation>
        <relation>
            <table>i_presupuestosprov</table>
            <field>h_presupuestosprov_codigo</field>
            <card>1M</card>
        </relation>
    </field>
	
    <field>
        <name>idpresupuestocli</name>
		<!--Identificador del presupuesto de cliente relacionado con la oferta-->        <alias>QT_TRANSLATE_NOOP("MetaData","Presupuesto")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>uint</type>
		<visiblegrid>false</visiblegrid>
      <relation>
            <table>presupuestoscli</table>
            <field>idpresupuesto</field>
            <card>M1</card>
        </relation>
    </field>
    
	<field>
        <name>codpresupuestocli</name>
		<!--Código asociado al presupuesto de cliente-->        <alias>QT_TRANSLATE_NOOP("MetaData","Presupuesto de cliente")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>12</length>
    </field>
    
	<field>
        <name>codproveedor</name>
		<!--Código del proveedor-->        <alias>QT_TRANSLATE_NOOP("MetaData","Cod.Proveedor")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>6</length>
        <relation>
            <table>proveedores</table>
            <field>codproveedor</field>
            <card>M1</card>
        </relation>
    </field>
	
    <field>
        <name>nombreproveedor</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Nombre Proveedor")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>100</length>
    </field>
    
	<field>
        <name>femision</name>
		<!--Fecha en la que se crea la solicitud de oferta-->
        <alias>QT_TRANSLATE_NOOP("MetaData","Fecha emisión")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>date</type>
    </field>
    
	<field>
        <name>frecepcion</name>
		<!--Fecha en la que se recibe la oferta-->        <alias>QT_TRANSLATE_NOOP("MetaData","Fecha recepción")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>date</type>
    </field>
    
	<field>
