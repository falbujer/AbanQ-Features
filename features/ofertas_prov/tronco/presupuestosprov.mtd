        <alias>QT_TRANSLATE_NOOP("MetaData","Estado")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>10</length>
        <optionslist>Pendiente,Enviada,Recibida,Anulada</optionslist>
		<default>Pendiente</default>
    </field>
	
	<field>
        <name>tipoemision</name>
		<!--Tipo de emisión-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo emisión")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>20</length>
        <optionslist>Fax,Correo electrónico,Teléfono,Otros</optionslist>
    </field>
	
