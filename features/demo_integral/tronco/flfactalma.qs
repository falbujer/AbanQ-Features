function demo_crearLote(datosArt:Array, cantidad:Number, idLinea:Number):String
{
	if (datosArt["referencia"] == "DET_X5") {
		cantidad = 500;
	}
	return this.iface.__crearLote(datosArt, cantidad, idLinea);
}
