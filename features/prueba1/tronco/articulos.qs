/** @class_definition prueba1 */
/////////////////////////////////////////////////////////////////
//// PRUEBA 1 ///////////////////////////////////////////////////
function prueba1_validateForm():Boolean
{
	var cursor:FLSqlCursor = this.cursor();
	var util:FLUtil = new FLUtil();

	if(!this.iface.__validateForm())
		return false;

	var res:Number = MessageBox.information(util.translate("scripts", "¿Desea Continuar?"), MessageBox.Yes, MessageBox.No);
	if (res != MessageBox.Yes) {
		return false;
	}

	return true;
}
