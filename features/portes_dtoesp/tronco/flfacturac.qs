//// PORTES /////////////////////////////////////////////////////
class portesDtoesp extends portes {
	function portesDtoesp( context ) { portes ( context ); }
	function netoVentasFacturaCli(curFactura:FLSqlCursor):Number {
		return this.ctx.portesDtoesp_netoVentasFacturaCli(curFactura);
	}
}
//// PORTES /////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////



/** @class_definition portesDtoesp */
//////////////////////////////////////////////////////////////////
//// PORTES + DTO ESPECIAL ////////////////////////////////////////////////

function portesDtoesp_netoVentasFacturaCli(curFactura:FLSqlCursor):Number
{
	return parseFloat(curFactura.valueBuffer("netosindtoesp"));
}

