<!DOCTYPE TMD>
<TMD>
	<name>sincrostockstienda</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Sincronización de stock de tiendas")</alias>
	
	<field>
		<name>idsincrost</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Id")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
	</field>
	
	<field>
		<name>idstock</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Stock")</alias>
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>stocks</table>
			<field>idstock</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	
	<field>
		<name>codtienda</name>
		<!-- Código-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>20</length>
		
		<relation>
			<table>tpv_tiendas</table>
			<field>codtienda</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	
	<field>
		<name>sincronizado</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","sincronizado")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>bool</type>
	</field>
</TMD>
