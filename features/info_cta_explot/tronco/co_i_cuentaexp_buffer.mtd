		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<partI>12</partI>
		<partD>2</partD>
		<default>0</default>
	</field>
	
	<field>
		<!-- Porcentaje sobre el VP para ambos ejercicios -->
		<name>portot</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","%")</alias>
