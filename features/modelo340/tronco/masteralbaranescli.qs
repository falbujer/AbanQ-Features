class modelo340 extends oficial
{
  function modelo340(context)
  {
    oficial(context);
  }
  function totalesFactura() {
    return this.ctx.modelo340_totalesFactura();
  }
}
//// MODELO340 //////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition modelo340 */
/////////////////////////////////////////////////////////////////
//// MODELO340 //////////////////////////////////////////////////
function modelo340_totalesFactura()
{
	var _i = this.iface;
	if (!_i.__totalesFactura()) {
		return false;
	}
	with(_i.curFactura) {
    setValueBuffer("claveoperacion340",  formfacturascli.iface.pub_commonCalculateField("claveoperacion340", this));
	}
  return true;
}
