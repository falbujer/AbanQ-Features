
/** @class_declaration incoterms */
/////////////////////////////////////////////////////////////////
//// INCOTERMS //////////////////////////////////////////////////
class incoterms extends oficial {
    function incoterms( context ) { oficial ( context ); }
    function datosAlbaran(curPedido, where, datosAgrupacion) {
		return this.ctx.incoterms_datosAlbaran(curPedido, where, datosAgrupacion);
	}
}
//// INCOTERMS //////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition incoterms */
/////////////////////////////////////////////////////////////////
//// INCOTERMS //////////////////////////////////////////////////
function incoterms_datosAlbaran(curPedido, where, datosAgrupacion)
{
	if (!this.iface.__datosAlbaran(curPedido, where, datosAgrupacion)) {
		return false;
	}
	with (this.iface.curAlbaran) {
		setValueBuffer("incoterm", curPedido.valueBuffer("incoterm"));
	}
	
	return true;
}
//// INCOTERMS //////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////
