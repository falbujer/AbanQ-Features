
/** @class_declaration multiempresa */
/////////////////////////////////////////////////////////////////
//// MULTIEMPRESA ///////////////////////////////////////////////
class multiempresa extends oficial {
	function multiempresa( context ) { oficial ( context ); }
	function dameDivisaEmpresa(codEjercicio) {
    return this.ctx.multiempresa_dameDivisaEmpresa(codEjercicio);
  }
}
//// MULTIEMPRESA ///////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition multiempresa */
/////////////////////////////////////////////////////////////////
//// MULTIEMPRESA ///////////////////////////////////////////////
function multiempresa_dameDivisaEmpresa(codEjercicio)
{
	return AQUtil.sqlSelect("ejercicios ej INNER JOIN empresa e ON ej.idempresa = e.id", "e.coddivisa", "ej.codejercicio = '" + codEjercicio + "'", "ejercicios");
}
//// MULTIEMPRESA ///////////////////////////////////////////////
/////////////////////////////////////////////////////////////////
